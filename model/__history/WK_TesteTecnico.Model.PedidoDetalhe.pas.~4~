unit WK_TesteTecnico.Model.PedidoDetalhe;

interface

type
  TItensPedido = class
    private
    FCodigo: Integer;
    FCodigoPedido: Integer;
    FQuantidade: Double;
    FValorUnitProduto: Double;
    FValorTotal: Double;
    public
      property detPedCodigo: Integer read FCodigo write FCodigo;
      property detProdCodigo: Integer read FCodigoPedido write FCodigoPedido;
      property detQuantidade: Double read FQuantidade write FQuantidade;
      property detValorUnit: Double read FValorUnitProduto write FValorUnitProduto;
      property detValorTotal: Double read FValorTotal write FValorTotal;

  end;

implementation

end.
